{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/api/index.php" },
    { "src": "/config/db.php", "dest": "/api/config/db.php" },
    { "src": "config/../vendor/autoload.php", "dest": "/api/config/../vendor/autoload.php" },
    { "src": "/vendor/(.*)", "dest": "/api/vendor/$1" }
  ]
}
